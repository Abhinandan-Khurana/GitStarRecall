# GitHub OAuth app client ID
VITE_GITHUB_CLIENT_ID=

# Must exactly match one callback URL configured in your GitHub OAuth app
# Example: http://localhost:5173/auth/callback
VITE_GITHUB_REDIRECT_URI=

# Backend endpoint that exchanges OAuth code + PKCE verifier for access token.
# This keeps client secret out of the browser.
VITE_GITHUB_OAUTH_EXCHANGE_URL=

# Embedding backend preference:
# - webgpu: try GPU backend first and fallback to wasm
# - wasm: force CPU/wasm only (kill switch)
VITE_EMBEDDING_BACKEND_PREFERRED=webgpu

# Optional embedding throughput tuning:
# - pool size is clamped to 1..2
# - worker batch size is clamped to 1..32
VITE_EMBEDDING_POOL_SIZE=2
VITE_EMBEDDING_WORKER_BATCH_SIZE=8
